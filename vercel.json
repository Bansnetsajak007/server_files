{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
        "routes": [
            {
                "src": "/download",
                "dest": "/index.js"
            },
                {
                    "src": "/.*",
                    "dest": "/"
                }
        ]
}